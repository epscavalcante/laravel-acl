{
  "name": "epscavalcante/laravel-acl",
  "description": "Advanced ACL for Laravel",
  "keywords": [
    "laravel",
    "acl",
    "groups",
    "roles"
  ],
  "homepage": "https://github.com/epscavalcante/laravel-acl",
  "license": "MIT",
  "authors": [
    {
      "name": "Eduardo",
      "email": "eduardo.ps.cavalcante@gmail.com"
    }
  ],
  "require-dev": {
    "phpunit/phpunit": "^7.5",
    "orchestra/testbench": "^3.1.0",
    "squizlabs/php_codesniffer": "^2.5.1",
    "phpmd/phpmd" : "^2.3.2",
    "mockery/mockery": "^1.0"
  },
  "require": {
    "php": "^7.2.5",
    "illuminate/support": "^6.20.43",
    "illuminate/container": "^6.20.43",
    "illuminate/database": "^6.20.43",
    "illuminate/events": "^6.20.43"
  },
  "autoload": {
    "psr-4": {
      "Signes\\Acl\\": "src/Acl/"
    }
  },
  "autoload-dev": {
    "classmap": [
